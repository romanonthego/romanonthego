{
    "name": "romanonthego.rocks",
    "version": "3.1.2",
    "description": "romanonthego.rocks front-end",
    "main": "index.js",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "heroku-postbuild": "yarn build",
        "test": "yarn lint",
        "lint": "./node_modules/.bin/eslint --quiet --ext .jsx --ext .js app/** || true",
        "lint:fix": "./node_modules/.bin/eslint --quiet --ext .jsx --ext .js app/** --fix",
        "clean-build-dir": "rm -rf ./build/*",
        "bump": "yarn version",
        "predev": "yarn clean-build-dir",
        "dev": "webpack --config webpack-configs/server.development.babel.js && yarn start",
        "dev:prerender": "DEV_PRERENDER=true webpack --config webpack-configs/server.development.babel.js && yarn start",
        "prebuild": "yarn clean-build-dir",
        "build": "yarn build:server && yarn build:browser",
        "build:server": "webpack --config webpack-configs/server.production.babel.js",
        "build:browser": "webpack --config webpack-configs/browser.production.babel.js",
        "preanalyze": "yarn clean-build-dir",
        "analyze": "NODE_ENV=production ANALYZE=true webpack --config webpack-configs/browser.production.babel.js",
        "start": "node ./build/server"
    },
